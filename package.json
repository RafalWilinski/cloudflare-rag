{
  "name": "cloudflare-rag",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "migrate:local": "npx drizzle-kit generate && wrangler d1 migrations apply cloudflare-rag --local",
    "migrate": "npx drizzle-kit generate && wrangler d1 migrations apply cloudflare-rag --remote"
  },
  "dependencies": {
    "@cloudflare/ai-utils": "^1.0.1",
    "@langchain/core": "^0.2.28",
    "@langchain/textsplitters": "^0.0.3",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/cloudflare": "^2.11.1",
    "@remix-run/cloudflare-pages": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "@tabler/icons-react": "^3.14.0",
    "@tailwindcss/typography": "^0.5.14",
    "ai": "^3.3.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "eventsource": "^2.0.2",
    "fetch-event-stream": "^0.1.5",
    "framer-motion": "^11.3.30",
    "isbot": "^4.1.0",
    "lucide-react": "^0.436.0",
    "pdfjs-dist": "^4.5.136",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-dropzone-esm": "^15.0.1",
    "react-markdown": "^9.0.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "ulidx": "^2.4.1",
    "unpdf": "^0.11.0",
    "vaul": "^0.9.1",
    "workers-ai-provider": "^0.0.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240821.1",
    "@remix-run/dev": "^2.11.1",
